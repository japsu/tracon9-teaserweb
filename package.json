{
  "name": "james-browserify-project-template",
  "version": "0.1.0",
  "main": "./server/server",
  "devDependencies": {
    "browserify": "2.12.x",
    "browserify-shim": "~3.0.4",
    "bower": "1.2.x",
    "coffeeify": "0.4.x",
    "james": "0.1.x",
    "james-jade-static": "0.1.x",
    "james-stylus": "0.1.x",
    "james-uglify": "0.1.x"
  },
  "dependencies": {
    "coffee-script": "1.6.x",
    "express": "~3.4.6"
  },
  "scripts": {
    "prepublish": "bower install && james build",
    "start": "coffee server/server.coffee"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./bower_components/jquery/jquery.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js"
  },
  "browserify-shim": {
    "bootstrap": {
      "exports": "$",
      "depends": [
        "jquery:$"
      ]
    }
  }
}
